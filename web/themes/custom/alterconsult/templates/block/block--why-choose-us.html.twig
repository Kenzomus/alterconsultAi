<div class="benefits-carousel">
  {% for item in items %}
    <div class="benefit-item">
      <div class="benefit-icon"><i class="{{ item.content.field_benefit_icon }}"></i></div>
      <h4>{{ item.content.field_benefit_title }}</h4>
      <p>{{ item.content.field_benefit_description }}</p>
    </div>
  {% endfor %}
</div>